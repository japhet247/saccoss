<div class="form-group"> 
    <label>Transactions from</label> 
    <input name="s_date_r" type="text" value="{{ s_date_r }} " class="form-control datepicker" > 
</div> 
<div class="form-group"> 
    <label>Transactions To</label> 
    <input name="e_date_r" type="text" value="{{ e_date_r }}" class="form-control datepicker" > 
</div>
<div class="form-group">
    <label>Narration</label>
    <div >
        <select name="narration_r"  class="chosen-select form-control">
        <option value="">--Select One--</option>
        {% for narration in narrations %}
        	<option value="{{ narration }}">{{ prepare_narration(narration) }}</option>
        {% endfor %}
      </select>
    </div>
</div>
<div class="form-group">
    <label>Municipal</label>
    <div >
        <select name="municipal_id_r"  class="chosen-select form-control">
        <option value="">--Select One--</option>
        {% for municipal in municipals %}
        	<option value="{{ municipal['municipal_id'] }}">{{ municipal['municipal_name'] }}</option>
        {% endfor %}
      </select>
    </div>
</div>